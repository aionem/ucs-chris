<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:primaryType="nt:unstructured" jcr:title="Footer Right Links" sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <well-multifield jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/well" fieldLabel="Footer Rght Links">
                        <items jcr:primaryType="nt:unstructured">
                            <utilNavItems jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/multifield" composite="{Boolean}true" fieldDescription="Util links" fieldLabel="Footer Right Links" required="{Boolean}false" name="utilNavItems">
                                <field jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container" emptyText="Util links" name="./utilNavItems">
                                    <items jcr:primaryType="nt:unstructured">
                                        <link jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" emptyText="Title of your Util Link" fieldLabel="Link Title" name="./link" />
                                        <linkTo jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/pathfield" rootPath="/content/ucs-exercise-issa" emptyText="Link to" fieldLabel="Link To" name="./linkTo" />
                                        <target jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/select" emptyText="Select Target" fieldLabel="Select Target" name="./target">
                                            <items jcr:primaryType="nt:unstructured">
                                                <option1 jcr:primaryType="nt:unstructured" text="New Tab" value="_blank" />
                                                <option2 jcr:primaryType="nt:unstructured" text="This Tab" value="#" />
                                            </items>
                                        </target>
                                    </items>
                                </field>
                            </utilNavItems>
                        </items>
                    </well-multifield>
                </items>
            </column>
        </items>
    </content>
</jcr:root>